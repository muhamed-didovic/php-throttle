        "illuminate/contracts": "5.4.*",
        "illuminate/http": "5.4.*",
        "illuminate/support": "5.4.*",
        "illuminate/container": "5.4.*",
        "larapack/dd": "1.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4" : {
            "MuhamedDidovic\\Throttle\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhamedDidovic\\Tests\\Throttle\\": "tests/"
